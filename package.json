{
	"name": "imgur-upload-server",
	"author": "mirasayon <mirasayon@ya.ru>",
	"version": "1.1.0",
	"description": "Simple Node.js server to upload images to https://imgur.com",
	"type": "module",
	"main": "./build/app.js",
	"scripts": {
		"build": "npx tsc -p ./tsconfig.json",
		"typedev": "npx tsc --watch -p ./tsconfig.json",
		"start": "node --env-file=.env build/app.js",
		"dev": "npx nodemon --env-file=.env build/app.js"
	},
	"keywords": [
		"imgur",
		"upload",
		"images"
	],
	"license": "MIT",
	"dependencies": {
		"commander": "^12.1.0",
		"express": "^4.19.2",
		"express-fileupload": "^1.5.0",
		"form-data": "^4.0.0",
		"got": "^14.4.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/express-fileupload": "^1.5.0",
		"@types/node": "^20.14.2",
		"nodemon": "^3.1.3",
		"typescript": "^5.4.5"
	}
}
